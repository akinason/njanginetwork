#: env/lib/python3.5/site-packages/django/contrib/postgres/apps.py:13
msgid "PostgreSQL extensions"
msgstr "Extensions PostgreSQL"

#: env/lib/python3.5/site-packages/django/contrib/postgres/fields/array.py:21
#: env/lib/python3.5/site-packages/django/contrib/postgres/forms/array.py:16
#: env/lib/python3.5/site-packages/django/contrib/postgres/forms/array.py:160
#, python-format
msgid "Item %(nth)s in the array did not validate: "
msgstr "L'élément n°%(nth)s du tableau n'est pas valide :"

#: env/lib/python3.5/site-packages/django/contrib/postgres/fields/array.py:22
msgid "Nested arrays must have the same length."
msgstr "Les tableaux imbriqués doivent être de même longueur."

#: env/lib/python3.5/site-packages/django/contrib/postgres/fields/hstore.py:14
msgid "Map of strings to strings/nulls"
msgstr "Correspondances clé/valeur (chaînes ou valeurs nulles)"

#: env/lib/python3.5/site-packages/django/contrib/postgres/fields/hstore.py:16
#, python-format
msgid "The value of \"%(key)s\" is not a string or null."
msgstr "La valeur de « %(key)s » n'est pas une chaîne, ni une valeur nulle."

#: env/lib/python3.5/site-packages/django/contrib/postgres/fields/jsonb.py:30
msgid "A JSON object"
msgstr "Un objet JSON"

#: env/lib/python3.5/site-packages/django/contrib/postgres/fields/jsonb.py:32
msgid "Value must be valid JSON."
msgstr "La valeur doit être de la syntaxe JSON valable."

#: env/lib/python3.5/site-packages/django/contrib/postgres/forms/hstore.py:16
msgid "Could not load JSON data."
msgstr "Impossible de charger les données JSON."

#: env/lib/python3.5/site-packages/django/contrib/postgres/forms/hstore.py:17
msgid "Input must be a JSON dictionary."
msgstr "Le contenu saisi doit être un dictionnaire JSON."

#: env/lib/python3.5/site-packages/django/contrib/postgres/forms/jsonb.py:19
#, python-format
msgid "'%(value)s' value must be valid JSON."
msgstr "La valeur « %(value)s » doit être de la syntaxe JSON valable."

#: env/lib/python3.5/site-packages/django/contrib/postgres/forms/ranges.py:16
msgid "Enter two valid values."
msgstr "Saisissez deux valeurs valides."

#: env/lib/python3.5/site-packages/django/contrib/postgres/forms/ranges.py:17
msgid "The start of the range must not exceed the end of the range."
msgstr "Le début de l'intervalle ne peut pas dépasser la fin de l'intervalle."

#: env/lib/python3.5/site-packages/django/contrib/postgres/forms/ranges.py:64
msgid "Enter two whole numbers."
msgstr "Saisissez deux nombres entiers."

#: env/lib/python3.5/site-packages/django/contrib/postgres/forms/ranges.py:70
msgid "Enter two numbers."
msgstr "Saisissez deux nombres."

#: env/lib/python3.5/site-packages/django/contrib/postgres/forms/ranges.py:76
msgid "Enter two valid date/times."
msgstr "Saisissez deux dates/heures valides."

#: env/lib/python3.5/site-packages/django/contrib/postgres/forms/ranges.py:82
msgid "Enter two valid dates."
msgstr "Saisissez deux dates valides."

#: env/lib/python3.5/site-packages/django/contrib/postgres/validators.py:14
#, python-format
msgid ""
"List contains %(show_value)d item, it should contain no more than "
"%(limit_value)d."
msgid_plural ""
"List contains %(show_value)d items, it should contain no more than "
"%(limit_value)d."
msgstr[0] ""
"La liste contient %(show_value)d élément, mais elle ne devrait pas en "
"contenir plus de %(limit_value)d."
msgstr[1] ""
"La liste contient %(show_value)d éléments, mais elle ne devrait pas en "
"contenir plus de %(limit_value)d."

#: env/lib/python3.5/site-packages/django/contrib/postgres/validators.py:21
#, python-format
msgid ""
"List contains %(show_value)d item, it should contain no fewer than "
"%(limit_value)d."
msgid_plural ""
"List contains %(show_value)d items, it should contain no fewer than "
"%(limit_value)d."
msgstr[0] ""
"La liste contient %(show_value)d élément, mais elle doit en contenir au "
"moins %(limit_value)d."
msgstr[1] ""
"La liste contient %(show_value)d éléments, mais elle doit en contenir au "
"moins %(limit_value)d."

#: env/lib/python3.5/site-packages/django/contrib/postgres/validators.py:31
#, python-format
msgid "Some keys were missing: %(keys)s"
msgstr "Certaines clés sont manquantes : %(keys)s"

#: env/lib/python3.5/site-packages/django/contrib/postgres/validators.py:32
#, python-format
msgid "Some unknown keys were provided: %(keys)s"
msgstr "Certaines clés inconnues ont été fournies : %(keys)s"

#: env/lib/python3.5/site-packages/django/contrib/postgres/validators.py:73
#, python-format
msgid ""
"Ensure that this range is completely less than or equal to %(limit_value)s."
msgstr ""
"Assurez-vous que cet intervalle est entièrement inférieur ou égal à "
"%(limit_value)s."

#: env/lib/python3.5/site-packages/django/contrib/postgres/validators.py:79
#, python-format
msgid ""
"Ensure that this range is completely greater than or equal to "
"%(limit_value)s."
msgstr ""
"Assurez-vous que cet intervalle est entièrement supérieur ou égal à "
"%(limit_value)s."
